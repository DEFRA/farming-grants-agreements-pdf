{
  "name": "farming-grants-agreements-pdf",
  "version": "0.0.0",
  "description": "farming-grants-agreements-pdf",
  "main": ".server/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.11.0"
  },
  "scripts": {
    "build": "npm run build:server",
    "build:server": "NODE_ENV=production babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"**/__fixtures__\" --ignore \"**/test-helpers\" --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "docker:dev": "NODE_ENV=development npm run server:watch",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint && npm run test",
    "commitlint": "commitlint --edit",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run setup:husky",
    "server:debug": "nodemon --exec tsx --enable-source-maps --inspect ./src",
    "server:watch": "nodemon --exec tsx --enable-source-maps ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict .",
    "test": "vitest run --exclude '**/contracts/**' --coverage --reporter=verbose",
    "test:watch": "vitest --exclude '**/contracts/**'",
    "test:contracts": "vitest run src/contracts",
    "test:contracts:consumer": "vitest run src/contracts/consumer",
    "test:contracts:provider": "echo 'No provider contract tests to run'"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "3.879.0",
    "@aws-sdk/client-sns": "3.879.0",
    "@aws-sdk/client-sqs": "3.879.0",
    "@babel/runtime": "^7.26.0",
    "@defra/cdp-auditing": "0.4.0",
    "@defra/hapi-secure-context": "0.1.3",
    "@defra/hapi-tracing": "1.22.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.0",
    "@hapi/jwt": "3.2.1",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "date-fns": "4.1.0",
    "dotenv": "^16.5.0",
    "global-agent": "3.0.0",
    "hapi-pino": "13.0.0",
    "hapi-pulse": "3.0.1",
    "https-proxy-agent": "7.0.6",
    "joi": "17.13.3",
    "mongo-locks": "3.0.2",
    "mongodb": "6.17.0",
    "node-fetch": "3.3.2",
    "pino": "9.13.1",
    "pino-pretty": "13.0.0",
    "puppeteer": "24.18.0",
    "sqs-consumer": "13.0.0",
    "undici": "7.18.2",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@commitlint/cli": "20.3.1",
    "@pact-foundation/pact": "16.0.0",
    "@vitest/coverage-v8": "4.0.16",
    "autoprefixer": "10.4.21",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "neostandard": "0.12.1",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "4.0.16",
    "vitest-fetch-mock": "0.4.5"
  }
}
