{
  "name": "farming-grants-agreements-pdf",
  "version": "0.0.0",
  "description": "farming-grants-agreements-pdf",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.11.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run setup:husky",
    "server:debug": "nodemon --exec \"node --env-file=.env --inspect-brk=0.0.0.0\" --ext js,json --legacy-watch ./src",
    "server:watch": "nodemon --exec \"node --env-file=.env --inspect=0.0.0.0\" --ext js,json --legacy-watch ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start": "NODE_ENV=production node --use-strict .",
    "test": "node --no-experimental-require-module ./node_modules/jest/bin/jest.js --testPathIgnorePatterns='/contracts/' --coverage --verbose --runInBand",
    "test:watch": "jest --watch",
    "test:contracts": "jest src/contracts"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "3.879.0",
    "@aws-sdk/client-sns": "3.879.0",
    "@aws-sdk/client-sqs": "3.879.0",
    "@defra/cdp-auditing": "0.4.0",
    "@defra/hapi-secure-context": "0.1.3",
    "@defra/hapi-tracing": "1.22.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.0",
    "@hapi/jwt": "3.2.1",
    "@jest/globals": "30.1.2",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "client-sns": "github:aws-sdk/client-sns",
    "client-sqs": "github:aws-sdk/client-sqs",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "date-fns": "4.1.0",
    "global-agent": "3.0.0",
    "hapi-pino": "13.0.0",
    "hapi-pulse": "3.0.1",
    "https-proxy-agent": "7.0.6",
    "joi": "17.13.3",
    "mongo-locks": "3.0.2",
    "mongodb": "6.17.0",
    "node-fetch": "3.3.2",
    "pino": "9.13.1",
    "pino-pretty": "13.0.0",
    "puppeteer": "24.18.0",
    "sqs-consumer": "13.0.0",
    "undici": "7.10.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "@pact-foundation/pact": "16.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-transform-import-meta": "2.3.3",
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "jest": "30.1.2",
    "jest-fetch-mock": "3.0.3",
    "neostandard": "0.12.1",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "vitest": "3.2.4",
    "vitest-fetch-mock": "0.4.5"
  }
}
